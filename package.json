    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate:generate": "typeorm migration:create",
    "migrate:run": "npx ts-node ./node_modules/typeorm/cli.js migration:run -d src/config/typeorm.config.ts",
    "migrate:revert": "npx ts-node ./node_modules/typeorm/cli.js migration:revert -d src/config/typeorm.config.ts"
  },
